select year,job_title,case_status,count(case_status) as popular from h1b_final where year ='2011' and case_status= 'CERTIFIED'group by year,job_title,case_status order by popular desc limit 10;

select year,job_title,case_status,count(case_status) as popular from h1b_final where year ='2012' and case_status= 'CERTIFIED'group by year,job_title,case_status order by popular desc limit 10;

select year,job_title,case_status,count(case_status) as popular from h1b_final where year ='2013' and case_status= 'CERTIFIED'group by year,job_title,case_status order by popular desc limit 10;

select year,job_title,case_status,count(case_status) as popular from h1b_final where year ='2014' and case_status= 'CERTIFIED'group by year,job_title,case_status order by popular desc limit 10;

select year,job_title,case_status,count(case_status) as popular from h1b_final where year ='2015' and case_status= 'CERTIFIED'group by year,job_title,case_status order by popular desc limit 10;

select year,job_title,case_status,count(case_status) as popular from h1b_final where year ='2016' and case_status= 'CERTIFIED'group by year,job_title,case_status order by popular desc limit 10;
